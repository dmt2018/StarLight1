  object qPriceHistory: TOraQuery
    SQL.Strings = (
      
        'SELECT a.n_id, a.price_date, a.price, a.spec_price, a.best_price' +
        ', a.discount'
      '  FROM price_list_all a'
      
        '  where n_id = :p_n_id and a.price_date between sysdate-12360 an' +
        'd sysdate and a.id_office = const_office'
      '  order by a.price_date desc')
    FetchAll = True
    Left = 160
    Top = 352
    ParamData = <
      item
        DataType = ftUnknown
        Name = 'p_n_id'
      end>
    object StringField1: TStringField
      FieldName = 'F_TYPE'
      Required = True
      Size = 50
    end
    object FloatField1: TFloatField
      FieldName = 'FT_ID'
      Required = True
    end
  end
